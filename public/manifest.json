{
  "name": "iLearn",
  "description": "A tracker to track and motivate to learn everyday. Create your targets, monitor your day to day goals to improve your knowledge on topics you like",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "My Learning Tracker"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "idle",
    "webNavigation",
    "unlimitedStorage",
    "notifications",
    "http://*/*", 
    "https://*/*"
],
"content_security_policy": "script-src 'self' 'sha256-fdAfMKZtCTRPKMRznGwonJHC++vCCrQS7XFnVrzSXAE=' 'sha256-YD8w9VLrdvqI5/tFVmpT+1337ly+e/JvPrkRxYZo/w4='; object-src 'self';"
, "background": 
{
    "scripts": ["background.js"]
}
}